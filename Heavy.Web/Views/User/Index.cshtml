@model IEnumerable<IdentityUser>
@using Microsoft.AspNetCore.Identity;
@{
    ViewData["Title"] = "Index";
}



<script>

    $("#delete").click(function myfunction() {
        if (confirm('确认删除？')) {
            return true;
        } else {
            return false;
        }
    })
</script>


<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">用户名</th>
            <th scope="col">emil</th>
            <th scope="col">操作</th>

        </tr>
    </thead>

    @foreach (var u in Model)
    {
        <tr>
            <th scope="col">@u.Id</th>
            <th scope="col">@u.UserName</th>
            <th scope="col">@u.Email</th>
            <th scope="col">

                <form asp-action="DeleteUser" asp-route-id="@u.Id" method="post">
                    <input type="submit" name="delete" id="delete" value="删除" />
                </form>
                <form asp-action="Edit" asp-route-id="@u.Id" method="get">
                    <input type="submit" name="delete" id="delete" value="修改" />
                </form>
            </th>
        </tr>

    }

</table>

<a asp-action="AddUser">添加</a>
